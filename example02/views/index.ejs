<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/style.css' />
  </head>
  <body>
    <div class="view">
      <h1><%= title %></h1>
      <p>Welcome to <%= title %></p>
    </div>
    <input type="button" value="click to fetchMessage" onclick="fetchMessage()">
  </body>
  <script type="text/javascript">
    
    function fetchMessage(){
      fetch('/api/message',{method:'GET'})
      .then(res=>res.json())
      .then(data=>{
        console.log(data)
      })
      .catch(err=>{
        console.log(err)
      })
    }
  </script>
</html>
